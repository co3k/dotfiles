{
  "languageserver": {
    "golang": {
      "command": "gopls",
      "filetypes": ["go"]
    },
    "efm": {
      "command": "efm-langserver",
      "args": [],
      "filetypes": ["markdown"]
    }
  },
  "coc.preferences.formatOnSaveFiletypes": ["prisma"],
