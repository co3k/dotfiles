[core]
	excludesfile = ~/.gitignore
	pager = less -r
	attributesfile = ~/.gitattributes
	editor = vim
[color]
	diff = auto
    ui = auto
[grep]
	lineNumber = true
[color "grep"]
	filename = green
	linenumber = yellow
	separator = white
	match = yellow reverse
[help]
	autocorrect = 1
[push]
	default = simple
